@model List<ResultFeatureDto>

<div class="feature">
    <div class="container-fluid">
        <div class="row align-items-center">
            @foreach(var item in Model)
            {
                <div class="col-lg-3 col-md-6 feature-col">
                    <div class="feature-content">
                        <i class="@item.Icon"></i>
                        <h2>@item.Title</h2>
                        <p>
                            @item.Description
                        </p>
                    </div>
                </div>
            }
        </div>
    </div>
</div>